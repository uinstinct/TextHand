{
  "extends": [ "eslint:recommended", "react-app", "react-app/jest" ],
  "overrides": [
    {
      "files": [ "**/*.js?(x)" ],
      "rules": {
        "indent": [ 2, 4 ],
        "linebreak-style": [ 2, "unix" ],
        "no-unused-vars": [
          1,
          {
            "vars": "all",
            "args": "after-used",
            "ignoreRestSiblings": false
          }
        ]
      }
    }
  ]
}